{
  "name": "Angular-Webpack-Es6-Seed",
  "version": "0.1.0",
  "description": "an express seed for AngularJs with ES6 and Webpack",
  "author": "Andrea Valentini <and.valentini94@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --inline --devtool eval --progress --colors --hot --host 0.0.0.0",
    "clean": "rm -rf ./build",
    "build:webpack": "webpack -p --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack && cp ./src/index.html ./build/"
  },
  "keywords": [
    "angular",
    "angularjs",
    "webpack",
    "es6",
    "javascript"
  ],
  "dependencies": {
    "angular": "^1.5.3",
    "angular-route": "^1.5.3"
  },
  "devDependencies": {
    "babel-core": "6.5.2",
    "babel-loader": "6.2.3",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "0.8.5",
    "font-awesome": "^4.5.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "json-loader": "0.5.4",
    "node-libs-browser": "1.0.0",
    "raw-loader": "0.5.1",
    "style-loader": "^0.13.1",
    "svgo": "^0.6.1",
    "svgo-loader": "^1.1.0",
    "url-loader": "0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  }
}
